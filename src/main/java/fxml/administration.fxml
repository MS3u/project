<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controls.AdminPanel">
   <children>
      <TableView fx:id="tableUsers" layoutX="14.0" layoutY="14.0" onMouseClicked="#tableClick" prefHeight="200.0" prefWidth="387.0">
        <columns>
          <TableColumn fx:id="tId" prefWidth="75.0" text="Id" />
          <TableColumn fx:id="tImie" prefWidth="75.0" text="Imię" />
            <TableColumn fx:id="tNazwisko" prefWidth="75.0" text="Nazwisko" />
            <TableColumn fx:id="tStanowisko" prefWidth="75.0" text="Stanowisko" />
            <TableColumn fx:id="tHaslo" prefWidth="75.0" text="Hasło" />
        </columns>
      </TableView>
      <Button layoutX="106.0" layoutY="259.0" mnemonicParsing="false" onAction="#addUser" text="Dodaj uzytkownika" />
      <Button layoutX="452.0" layoutY="259.0" mnemonicParsing="false" onAction="#deleteUser" text="Usuń" />
      <Button layoutX="261.0" layoutY="259.0" mnemonicParsing="false" onAction="#editUser" prefHeight="25.0" prefWidth="99.0" text="Edycja" />
      <TextField fx:id="tfImie" layoutX="421.0" layoutY="22.0" />
      <TextField fx:id="tfNazwisko" layoutX="421.0" layoutY="64.0" />
      <TextField fx:id="tfStanowisko" layoutX="421.0" layoutY="114.0" />
      <TextField fx:id="tfHaslo" layoutX="421.0" layoutY="162.0" />
   </children>
</AnchorPane>
